<service>
    <id>PAPSS-Configuration-Service</id>
    <name>PAPSS-Configuration-Service</name>
    <description>PAPSS Configuration Service for PAPSS REST API Middleware</description>
    <!-- <env name="C:\Var\Papss\lib" value="%BASE%"/> -->
    <env name="PAPSS_HOME" value="%BASE%"/>
    <executable>java</executable>
    <arguments>-Xmx512m -jar "%BASE%\PAPSS-Configuration-Service-1.0.jar" --spring.cloud.config.server.native.search-locations=file:///C:/Var/Papss/config</arguments>
    <logmode>rotate</logmode>
</service>
